
@{
    ViewBag.Title = "_loginLayout";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/layout.css" rel="stylesheet" />

</head>
<body style="margin:0 auto; padding-top:0; ">
    <div class="container" style="background:white;padding-left:0!important; margin-bottom:-20px;">
        <div class="row col-md-2 text-center navLeft" style="margin:0; padding-left:0!important;">
            <div class="row text-center">
               <div class="col-md-12 upText"><p>Smerrebred</p></div>
                <div class="col-md-12 listat">
                    <div class="row">
                        <ul>
                            <li><a class="btn" href="">Home</a></li>
                            <li><a  class="btn" href="">About</a></li>
                            <li><a  class="btn"href="">Contact</a></li>
                            <li><a class="btn" href="">Viedos</a></li>
                          
                        </ul>
                    </div>
                </div>
               
            </div>
        </div>

        <div class="col-md-10" style="background:white">
            @RenderBody()
            <hr />
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
